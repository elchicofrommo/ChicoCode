{
  "presets": 
  	[
  		["@babel/preset-env" ],
  		"@babel/typescript",
  		"@babel/preset-react"
  	],
  "plugins": 
    [
 
  		"@babel/plugin-transform-runtime", 


		"@babel/proposal-object-rest-spread",
		 ["@babel/plugin-proposal-decorators", { legacy: true }],
        ["@babel/plugin-proposal-class-properties", { loose: true }],
	  	'react-hot-loader/babel', 
  	]
}
