{
	"name": "chicoexpress",
	"version": "0.0.2",
	"description": "base config for chicoexpress set up",
	"main": "server.js",
	"jest": {
		"testEnvironment": "node",
		"setupFiles": [
			"dotenv/config"
		],
		"preset": "@shelf/jest-mongodb"
	},
	"scripts": {
		"clean": "rimraf bin/",
		"webpack-build": "webpack --config webpack.config.js",
		"webpack-build-server": "webpack --config webpack.config.server.js",
		"babel-build": "babel ./src/server --out-dir bin/server --source-maps",
		"dev-build": "rimraf bin/ && babel ./src --out-dir bin/ --copy-files --source-maps",
		"build": "npm run clean && npm run webpack-build && npm run babel-build",
		"build-and-watch": "npm run clean && npm run webpack-build --watch & npm run babel-build --watch",
		"start": "nodemon  -r dotenv/config --inspect bin/server",
		"start-webpack": "node -r dotenv/config --inspect bin/server/server",
		"test": "jest --detectOpenHandles",
		"test-url": "jest shorturl-dao.test.js"
	},
	"dependencies": {
		"@babel/runtime": "^7.8.7",
		"@fortawesome/fontawesome-svg-core": "^1.2.26",
		"@fortawesome/free-solid-svg-icons": "^5.12.0",
		"@fortawesome/react-fontawesome": "^0.1.8",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.4.0",
		"@testing-library/user-event": "^7.2.1",
		"body-parser": "^1.19.0",
		"bootstrap": "^4.4.1",
		"chalk": "^2.4.2",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dns": "^0.2.2",
		"express": "^4.17.1",
		"express-fileupload": "^1.1.6",
		"express-winston": "^3.1.0",
		"http-errors": "~1.6.3",
		"log4js": "^4.1.1",
		"mongodb": "^3.3.3",
		"mongoose": "^5.9.3",
		"morgan": "~1.9.1",
		"node-sass": "^4.13.1",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-redux": "^7.2.0",
		"react-scripts": "3.3.1",
		"redux": "^4.0.5",
		"winston": "^3.2.1"
	},
	"engines": {
		"node": "10.17.0"
	},
	"repository": {
		"type": "git",
		"url": "https://hyperdev.com/#!/project/welcome-project"
	},
	"keywords": [
		"node",
		"hyperdev",
		"express",
		"Babel"
	],
	"license": "MIT",
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.8.7",
		"@babel/plugin-transform-react-jsx": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.8.3",
		"@babel/preset-env": "^7.8.7",
		"@babel/preset-react": "^7.8.3",
		"@babel/register": "^7.8.6",
		"@shelf/jest-mongodb": "^1.1.3",
		"babel-polyfill": "^6.26.0",
		"copy-webpack-plugin": "^5.1.1",
		"dotenv": "^8.2.0",
		"html-webpack-plugin": "^4.0.0-beta.8",
		"jest": "^25.1.0",
		"jest-babel": "^1.0.1",
		"nodemon": "^2.0.2",
		"rimraf": "^2.6.3",
		"webpack": "^5.0.0-beta.14",
		"webpack-cli": "^3.3.11"
	}
}
