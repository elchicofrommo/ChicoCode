{
  "name": "chico_express",
  "version": "1.0.0",
  "description": "A Chico Express node.js project shell",
  "scripts": {
    "build": "rimraf bin/ && babel ./ --out-dir bin/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node --inspect bin/www",
    "start_debug": "npm run build && node --inspect bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elchicofrommo/chicoexpress.git"
  },
  "keywords": [
    "Node.js",
    "Babel",
    "Express"
  ],
  "author": "Mario Hernandez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elchicofrommo/chicoexpress/issues"
  },
  "homepage": "https://github.com/elchicofrommo/chicoexpress#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-winston": "^3.1.0",
    "http-errors": "~1.6.3",
    "log4js": "^4.1.1",
    "morgan": "~1.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/preset-stage-2": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.3"
  }
}
